<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Clickly </title>
    <link href="~/images/favicon.png" rel="icon" type="image/svg"/>
    <script type="importmap"></script>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="" href="~/css/tailwind.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Clickly.styles.css" asp-append-version="true" />
</head>
<body>
    
    <div id="wrapper">
        @* TopBar *@
        <header class="z-\[10000\] h-[--m-top] fixed-0 left-0 w-full items-center bg-white/80 sky-50 backdrop-blur-xl bordor-b border-slate-200>
            <div class="2xl:w-[--w-side]-lg:w-[--w-side-sm]">
                <div class="flex itemsalign-content-center gap-1">

                </div>
                <div>
                </div>
            </div>
            

        </header>
        @RenderBody
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <script src="~/js/uikit.min.js"></script>
    <script src="~/js/simplebar.js"></script>
    <script src="~/js/script.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
